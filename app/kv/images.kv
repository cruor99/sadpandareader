
<GalleryImage>:
    size_hint: None, None
    width: self.parent.width
    height: self.parent.height
    allow_stretch: True
    keep_ratio: True
    do_translation: False


<GalleryCarousel>:
    id: "galcarousel"
    loop: True
    #on__offset: self.startmove()

<GalleryContainerLayout>:
    id: "galcontainer"
